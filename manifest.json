{
  "manifest_version": 3,
  "name": "Asobistore マイページ注文履歴 商品名表示拡張 ",
  "version": "1.0.0",
  "permissions": [
  "scripting",
  "activeTab"
  ],
  "host_permissions": [
    "https://shop.asobistore.jp/*",
    "http://shop.asobistore.jp/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://shop.asobistore.jp/mypage/orderhistory*"],
      "js": ["content_script.js"]
    }
  ]
}

